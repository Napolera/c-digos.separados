#define pinRele_A 3
#define pinRele_B 8

void setup() {
pinMode(pinRele_1, OUTPUT);
pinMode(pinRele_2, OUTPUT);
}

void loop() {
digitalWrite(pinRele_A, LOW);
delay(1000);
digitalWrite(pinRele_A, HIGH);
delay(1000);
digitalWrite(pinRele_B, LOW);
delay(1000);
digitalWrite(pinRele_B, HIGH);
delay(1000);
}
